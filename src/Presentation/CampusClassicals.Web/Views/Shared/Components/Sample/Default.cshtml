@model IEnumerable<string>

<div style="font-size:18px; font-family:Verdana, Tahoma, 'Segoe UI', Geneva, sans-serif">
    <div style="color:orangered; font-weight:bold; ">
        See below for the categories:
    </div>
    <div>
        <ul>
            @foreach (string s in Model)
            {
                <li>@s</li>
            }
        </ul>
    </div>
</div>
